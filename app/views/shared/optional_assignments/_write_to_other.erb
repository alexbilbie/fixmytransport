<%- @campaign.write_to_other_assignments.each do |assignment| %>
  <%- if assignment.status == :complete %>
    <div class='assignment'>
      <%= link_to(t(:write_again, :name => assignment.data[:name]), new_campaign_outgoing_message_path(@campaign, :assignment_id => assignment.id)) %>
    </div>
  <%- end %>
<%- end %>